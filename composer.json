{
  "name": "inmarelibero/gitignore-checker",
  "description": "A PHP library to check if paths are ignored by git",
  "type": "library",
  "keywords": [
    "Git",
    "Gitignore",
    "VCS"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*"
  },
  "autoload": {
    "psr-4": {
      "Inmarelibero\\GitIgnoreChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inmarelibero\\GitIgnoreChecker\\Tests\\": "tests/"
    }
  }
}